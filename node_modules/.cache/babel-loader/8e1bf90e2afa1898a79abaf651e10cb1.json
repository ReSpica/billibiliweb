NA,0BAMA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,QAAA,CAAA,EAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,cAAA,CAAA,GAAA;AACA,iBAFA,CADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA,KAVA;AAWA;AACA,IAAA,cAZA,0BAYA,CAZA,EAYA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,SADA,GACA,CAAA,CAAA,GAAA,CAAA,UAAA,IAAA,EAAA,KAAA,EAAA;AACA,kBAAA,IAAA,CAAA,IAAA,GAAA,EAAA;AACA,kBAAA,IAAA,CAAA,IAAA,GAAA,CAAA;AACA,kBAAA,IAAA,CAAA,QAAA,GAAA,EAAA,CAHA,CAIA;;AACA,kBAAA,IAAA,CAAA,QAAA,GAAA,KAAA;AACA,kBAAA,IAAA,CAAA,OAAA,GAAA,KAAA;AACA,yBAAA,IAAA;AACA,iBARA,CADA;AAUA,gBAAA,MAAA,CAAA,UAAA,GAAA,SAAA,CAVA,CAWA;;AACA,gBAAA,MAAA,CAAA,aAAA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAaA,KAzBA;AA0BA;AACA,IAAA,aA3BA,2BA2BA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,YADA,GACA,MAAA,CAAA,YAAA,EADA;AAAA;AAAA,uBAEA,MAAA,CAAA,YAAA,CAAA,GAAA,EAAA;AACA,kBAAA,IAAA,EAAA,YAAA,CAAA,IADA;AAEA,kBAAA,QAAA,EAAA,YAAA,CAAA;AAFA,iBAAA,CAFA;;AAAA;AAEA,gBAAA,GAFA;;AAMA,sCAAA,YAAA,CAAA,IAAA,EAAA,IAAA,8CAAA,GAAA;;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,YAAA,EAPA,CAQA;;AACA,oBAAA,GAAA,CAAA,MAAA,GAAA,YAAA,CAAA,QAAA,EAAA;AACA,kBAAA,YAAA,CAAA,QAAA,GAAA,IAAA;AACA;;AAXA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA,KAvCA;AAwCA;AACA,IAAA,YAzCA,0BAyCA;AACA,UAAA,YAAA,GAAA,KAAA,UAAA,CAAA,KAAA,MAAA,CAAA;AACA,aAAA,YAAA;AACA,KA5CA;AA6CA;AACA,IAAA,MA9CA,oBA8CA;AAAA;;AACA;AACA,UAAA,YAAA,GAAA,KAAA,YAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,UAAA,YAAA,CAAA,KAAA,GAAA,WAAA;AACA,MAAA,YAAA,CAAA,OAAA,GAAA,KAAA;AACA,MAAA,UAAA,CAAA,YAAA;AACA,QAAA,YAAA,CAAA,IAAA,IAAA,CAAA;;AACA,QAAA,MAAA,CAAA,aAAA;AACA,OAHA,EAGA,IAHA,CAAA;AAIA;AAvDA,GARA;AAiEA,EAAA,KAAA,EAAA;AACA,IAAA,MADA,oBACA;AACA,WAAA,aAAA;AACA;AAHA,GAjEA;AAsEA,EAAA,UAAA,EAAA;AACA,IAAA,MAAA,EAAA,MADA;AAEA,IAAA,MAAA,EAAA;AAFA,GAtEA;AA0EA,EAAA,OA1EA,qBA0EA;AACA,SAAA,YAAA;AACA,SAAA,YAAA;AACA;AA7EA,CAAA","sourcesContent":["<template>\r\n  <div class=\"bgc-w\">\r\n    <NavBar :model=\"model\"></NavBar>\r\n\r\n    <!-- 滑动导航 -->\r\n    <van-tabs\r\n      v-model=\"active\"\r\n      swipeable\r\n      animated\r\n    >\r\n      <van-tab\r\n        v-for=\"(item1,i1) in categories\"\r\n        :title=\"item1.title\"\r\n        :key=\"i1\"\r\n      >\r\n\r\n        <!-- 循环了的item -->\r\n        <div class=\"detailParent d-flex jc-around\">\r\n          <Detail\r\n            class=\"detailitem\"\r\n            v-for=\"(item2,i2) in item1.list\"\r\n            :key=\"i2\"\r\n            :model=\"item2\"\r\n          />\r\n        </div>\r\n\r\n        <!-- 上拉加载更多 -->\r\n        <van-list\r\n          :immediate-check=\"false\"\r\n          v-model=\"item1.loading\"\r\n          :finished=\"item1.finished\"\r\n          finished-text=\"没有更多了\"\r\n          @load=\"onLoad\"\r\n        />\r\n\r\n      </van-tab>\r\n    </van-tabs>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n//api\r\nimport { userinfo, category, detail } from \"@/network/home.js\";\r\n//组件\r\nimport NavBar from \"@/components/commom/NavBar.vue\";\r\nimport Detail from \"@/views/detail.vue\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      model: [],\r\n      categories: {},\r\n      active: 0\r\n    };\r\n  },\r\n  methods: {\r\n    async userinfoData() {\r\n      const res = await userinfo({}).then(res => {\r\n        this.model = res[0];\r\n      });\r\n    },\r\n    async categoryData() {\r\n      await category({}).then(res => {\r\n        this.changeCategory(res);\r\n      });\r\n    },\r\n    //循环添加个list进去\r\n    async changeCategory(e) {\r\n      const category1 = e.map((item, index) => {\r\n        item.list = [];\r\n        item.page = 0;\r\n        item.pagesize = 10;\r\n        //上拉加载的flag\r\n        item.finished = false;\r\n        item.loading = false;\r\n        return item;\r\n      });\r\n      this.categories = category1;\r\n      //刚加完空的list后，获取首页actyive：0的数据，且加到list中\r\n      this.selectArticle();\r\n    },\r\n    //面对不同id，list的值\r\n    async selectArticle() {\r\n      const categoryitem = this.categoryitem();\r\n      const res = await detail(categoryitem._id, {\r\n        page: categoryitem.page,\r\n        pagesize: categoryitem.pagesize\r\n      });\r\n      categoryitem.list.push(...res);\r\n      console.log(categoryitem)\r\n    //   categoryitem.loading = true;\r\n    if(res.length <categoryitem.pagesize){\r\n        categoryitem.finished = true;\r\n    }\r\n    },\r\n    //监听active，获取当前id\r\n    categoryitem() {\r\n      const categoryitem = this.categories[this.active];\r\n      return categoryitem;\r\n    },\r\n    //上拉加载\r\n    onLoad() {\r\n      //知道一下我上拉的是那个category\r\n      const categoryitem = this.categoryitem()\r\n      console.log('拉动的是：'+categoryitem.title+'，准备page++');\r\n      categoryitem.loading =false;\r\n      setTimeout(() => {\r\n        categoryitem.page += 1;\r\n        this.selectArticle();\r\n      }, 1000);\r\n    }\r\n  },\r\n  watch: {\r\n    active() {\r\n      this.selectArticle();\r\n    }\r\n  },\r\n  components: {\r\n    NavBar,\r\n    Detail\r\n  },\r\n  created() {\r\n    this.userinfoData();\r\n    this.categoryData();\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.detailParent {\r\n  flex-wrap: wrap;\r\n  .detailitem {\r\n    width: 45%;\r\n    margin: 5px 0;\r\n  }\r\n}\r\n</style>"],"sourceRoot":"src/views"}]}